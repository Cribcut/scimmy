  "homepage": "https://scimmyjs.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scimmyjs/scimmy.git"
  },
  "bugs": {
    "url": "https://github.com/scimmyjs/scimmy/issues"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "chalk": "^5.2.0",
    "classy-template": "^1.2.0",
    "jsdoc": "^4.0.2",
    "minimist": "^1.2.8",
    "mocha": "^10.2.0",
    "rollup": "^3.20.2",
    "typescript": "^5.0.2"
  }
}
